#import <Foundation/Foundation.h>

@interface GuideQuestion : NSObject

/**
  引导问题内容
 */
@property (nonatomic,copy) NSString *QuestionContent;


/**
 问题对应健康计划名称（选择答案“是”时对应的健康计划名称）    
 */
@property (nonatomic,copy) NSString *PlanNameYes;

/**
 引导问题排序
 */
@property(nonatomic,assign)NSInteger Sort;

/**
 问题对应健康计划编号（选择答案“是”时对应的健康计划编号）
 */
@property(nonatomic,assign)NSInteger PlanIDYes;

/**
 问题对应健康计划编号（选择答案“否”时对应的健康计划编号）
 */
@property(nonatomic,assign)NSInteger PlanIDNo;

/**
 引导问题编号
 */
@property(nonatomic,assign)NSInteger GuideQuestionID;

//问题对应健康计划名称（选择答案“否”时对应的健康计划名称）
@property (nonatomic, copy) NSString *PlanNameNo;

/**
 引导问题图片
 */
@property (nonatomic, copy) NSString *QuestionImg;
@end
