-(void)awakeFromNib{
    
    [super awakeFromNib];
    self.backgroundColor = [UIColor getColor:@"eff2f5"];
    self.lienView.backgroundColor=[UIColor getColor:@"EBF0F4"];
    self.backgroundColor = [UIColor getColor:@"edf8ff"];
    
    self.titleLab.textColor=[UIColor getColor:@"2C4667"];
    
    
    CAShapeLayer *maskLayer = [[CAShapeLayer alloc] init];
    UIBezierPath *corPath = [UIBezierPath bezierPathWithRoundedRect:CGRectMake(0, 0, KScreenWidth-20, 45) byRoundingCorners:UIRectCornerTopLeft | UIRectCornerTopRight cornerRadii:CGSizeMake(5, 5)];
    maskLayer.frame = corPath.bounds;
    maskLayer.path=corPath.CGPath;
    self.backView.layer.mask=maskLayer;
    
}


